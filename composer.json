{
    "name": "yampi/moip-sdk-php",
    "description": "Cliente em PHP para integração server-side com APIs Moip v2",
    "keywords": ["libraries", "moip", "intermediary", "checkout", "marketplace", "yampi"],
    "license": "MIT",
    "type": "libraries",
    "require": {
        "php": ">=5.5",
        "rmccue/requests": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.35 || ^5.7 || ^6.4",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Moip\\": "src/"
        },
        "files": [
            "src/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Moip\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
